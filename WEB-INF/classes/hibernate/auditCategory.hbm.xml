<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.virtusa.akura.api.dto.AuditCategory" table="AUDIT_CATEGORY">
		<id name="auditCategoryId" type="int" column="AUDIT_CATEGORY_ID">
			<generator class="increment" />
		</id>
		<property name="auditCategoryDescription" type="string">
			<column name="DESCRIPTION" length="45" />
		</property>
		<property name="param0Description" type="string">
			<column name="PARAM0_DESCRIPTION" length="45" />
		</property>
		<property name="param1Description" type="string">
			<column name="PARAM1_DESCRIPTION" length="45" />
		</property>
		<property name="param2Description" type="string">
			<column name="PARAM2_DESCRIPTION" length="45" />
		</property>
		<property name="param3Description" type="string">
			<column name="PARAM3_DESCRIPTION" length="45" />
		</property>
		<property name="param4Description" type="string">
			<column name="PARAM4_DESCRIPTION" length="45" />
		</property>
		<property name="param5Description" type="string">
			<column name="PARAM5_DESCRIPTION" length="45" />
		</property>
		<property name="param6Description" type="string">
			<column name="PARAM6_DESCRIPTION" length="45" />
		</property>
		<property name="param7Description" type="string">
			<column name="PARAM7_DESCRIPTION" length="45" />
		</property>
		<property name="param8Description" type="string">
			<column name="PARAM8_DESCRIPTION" length="45" />
		</property>
		<property name="param9Description" type="string">
			<column name="PARAM9_DESCRIPTION" length="45" />
		</property>
		<property name="audittedTable" type="string">
			<column name="AUDITTED_TABLE" length="45" />
		</property>
	</class>
</hibernate-mapping>